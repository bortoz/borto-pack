{
  "context-menu": {
    ".borto-log-message, .borto-testcase-box": [
      {
        "label": "Copy",
        "command": "borto-pack:copy"
      },
      {
        "label": "Search on google",
        "command": "borto-pack:search"
      }
    ],
    ".borto-log-file[data-path]": [
      {
        "label": "Open file",
        "command": "borto-pack:open-file"
      },
      {
        "label": "Show in Explorer",
        "command": "borto-pack:open-explorer"
      }
    ],
    ".borto-testcase.resolved": [
      {
        "label": "Open testcase",
        "command": "borto-pack:open-testcase"
      }
    ]
  },
  "menu": [
    {
      "label": "Packages",
      "submenu": [
        {
          "label": "Borto's Package",
          "submenu": [
            {
              "label": "Beautify",
              "command": "borto-pack:beautify"
            },
            {
              "label": "Compile",
              "command": "borto-pack:build"
            },
            {
              "label": "Compile and Run",
              "command": "borto-pack:build-and-run"
            },
            {
              "label": "Run",
              "command": "borto-pack:run"
            },
            {
              "label": "Multi Run",
              "command": "borto-pack:multi-run"
            },
             {
              "label": "Debug",
              "command": "borto-pack:debug"
            }
          ]
        }
      ]
    }
  ]
}
